{
  "name": "rabbitr1-prompts",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "concurrently \"vite\" \"npm run server:dev\"",
    "server:dev": "cd server && npm run dev",
    "start": "concurrently \"npm run serve\" \"npm run server:start\"",
    "server:start": "cd server && npm run start",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "serve": "vite preview --port 3000 --host"
  },
