    "@firebase/auth": "^1.8.1",
    "@tanstack/react-query": "^5.60.4",
    "@types/crypto-js": "^4.2.2",
    "@types/lodash": "^4.17.13",
    "@types/react-helmet-async": "^1.0.1",
    "axios": "^1.7.7",
    "crypto-js": "^4.2.0",
    "date-fns": "^4.1.0",
    "firebase": "^11.0.2",
    "framer-motion": "^11.11.17",
    "lodash": "^4.17.21",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-helmet-async": "^2.0.5",
